        android:id="@+id/btn_num5"
        android:layout_width="@dimen/btn_num_width"
        android:layout_height="@dimen/btn_num_height"
        android:background="@drawable/shape_button_ripple_gray"
        android:text="5"
        android:textColor="@color/white"
        android:textSize="@dimen/text_size_num"
        app:layout_constraintBottom_toTopOf="@+id/guideline25"
        app:layout_constraintEnd_toStartOf="@+id/guideline30"
        app:layout_constraintStart_toStartOf="@+id/guideline29"
        app:layout_constraintTop_toTopOf="@+id/guideline23" />

    <Button
        android:id="@+id/btn_num6"
        android:layout_width="@dimen/btn_num_width"
        android:layout_height="@dimen/btn_num_height"
        android:background="@drawable/shape_button_ripple_gray"
        android:text="6"
        android:textColor="@color/white"
        android:textSize="@dimen/text_size_num"
